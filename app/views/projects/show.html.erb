<% provide(:title, @project.title) %><h1><%= @project.title %></h1><p><%= @project.description %></p><p>Due on <%= @project.format_date %></p><h2>Unfinished Tasks</h2><table class='table'>  <thead>    <tr>      <th class='col-md-4'>Title</th>      <th class='col-md-6'>Description</th>      <th class='col-md-2'>Status</th>    </tr>  </thead>  <tbody>    <% @unfinished_tasks.each do |task| %>      <tr>        <td><%= task.title %></td>        <td><%= task.description %></td>        <td><%= link_to 'Finish', finish_project_task_path(@project, task), method: :put %></td>      </tr>    <% end %>  </tbody></table><h2>Finished Tasks</h2><table class='table'>  <thead>    <tr>      <th class='col-md-4'>Title</th>      <th class='col-md-6'>Description</th>      <th class='col-md-2'>Finished At</th>    </tr>  </thead>  <tbody>    <% @finished_tasks.each do |task| %>      <tr>        <td><%= task.title %></td>        <td><%= task.description %></td>        <td><%= task.updated_at.strftime('%B %e, %Y') %></td>      </tr>    <% end %>  </tbody></table><br><%= link_to 'Back', projects_path, class: 'btn btn-primary' %><%= link_to 'Edit', edit_project_path, class: 'btn btn-primary' %><%= link_to 'New Task', new_project_task_path(@project), class: 'btn btn-primary' %>